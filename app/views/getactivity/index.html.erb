<h1>Listing transactions</h1>

<table>
<tr>
<th>T id</th>
<th>T value</th>
<th>T value_date</th>
<th>T value_currency</th>
<th>T transaction_type</th>
<th>T transaction_type_code</th>
<th>T provider_org</th>
<th>T provider_org_ref</th>
<th>T provider_org_type</th>
<th>T receiver_org</th>
<th>T receiver_org_ref</th>
<th>T receiver_org_type</th>
<th>T description</th>
<th>T transaction_date</th>
<th>T transaction_date_iso</th>
<th>T flow_type</th>
<th>T flow_type_code</th>
<th>T aid_type</th>
<th>T aid_type_code</th>
<th>T finance_type</th>
<th>T finance_type_code</th>
<th>T tied_status_code</th>
<th>T disbursement_channel_code</th>
<th>A_id</th>
</tr>
<% @t.each do |t| %>
<tr>
	<td><%= t[:t_id] %></td>
	<td><%= t[:a_id] %></td>
	<td><%= t[:value] %></td>
<td><%= t[:t_value_date] %></td>
<td><%= t[:t_value_currency] %></td>
<td><%= t[:t_transaction_type] %></td>
<td><%= t[:t_transaction_type_code] %></td>
<td><%= t[:t_provider_org] %></td>
<td><%= t[:t_provider_org_ref] %></td>
<td><%= t[:t_provider_org_type] %></td>
<td><%= t[:t_receiver_org] %></td>
<td><%= t[:t_receiver_org_ref] %></td>
<td><%= t[:t_receiver_org_type] %></td>
<td><%= t[:t_description] %></td>
<td><%= t[:t_transaction_date] %></td>
<td><%= t[:t_transaction_date_iso] %></td>
<td><%= t[:t_flow_type] %></td>
<td><%= t[:t_flow_type_code] %></td>
<td><%= t[:t_aid_type] %></td>
<td><%= t[:t_aid_type_code] %></td>
<td><%= t[:t_finance_type] %></td>
<td><%= t[:t_finance_type_code] %></td>
<td><%= t[:t_tied_status_code] %></td>
<td><%= t[:t_disbursement_channel_code] %></td>


</tr>
<% end %>
</table>

<br />

